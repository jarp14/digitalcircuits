<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated digitaldomain -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="digital.presentation.DigitalModelWizardID"
            name="%_UI_DigitalModelWizard_label"
            class="digital.presentation.DigitalModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/DigitalModelFile.gif">
         <description>%_UI_DigitalModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated digitaldomain -->
      <editor
            id="digital.presentation.DigitalEditorID"
            name="%_UI_DigitalEditor_label"
            icon="icons/full/obj16/DigitalModelFile.gif"
            extensions="digital"
            class="digital.presentation.DigitalEditor"
            contributorClass="digital.presentation.DigitalActionBarContributor">
      </editor>
   </extension>

</plugin>
